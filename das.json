{
  "id": null,
  "title": "AWX Dashboard Modern Full",
  "uid": "awx-full",
  "version": 1,
  "schemaVersion": 36,
  "tags": ["awx", "ansible", "modern"],
  "timezone": "browser",
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "Total Jobs",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "datasource": "Prometheus",
      "targets": [
        { "expr": "sum(awx_jobs_total)", "refId": "A" }
      ]
    },
    {
      "type": "stat",
      "title": "Successful Jobs",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "datasource": "Prometheus",
      "targets": [
        { "expr": "sum(awx_status_total{status=\"successful\"})", "refId": "A" }
      ]
    },
    {
      "type": "stat",
      "title": "Failed Jobs",
      "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
      "datasource": "Prometheus",
      "targets": [
        { "expr": "sum(awx_status_total{status=\"failed\"})", "refId": "A" }
      ]
    },
    {
      "type": "stat",
      "title": "Total Projects",
      "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
      "datasource": "Prometheus",
      "targets": [
        { "expr": "sum(awx_projects_total)", "refId": "A" }
      ]
    },
    {
      "type": "timeSeries",
      "title": "Jobs Over Time",
      "gridPos": { "x": 0, "y": 4, "w": 24, "h": 8 },
      "datasource": "Prometheus",
      "targets": [
        { "expr": "rate(awx_jobs_total[5m])", "refId": "A" }
      ]
    },
    {
      "type": "gauge",
      "title": "Capacity Used %",
      "gridPos": { "x": 0, "y": 12, "w": 6, "h": 5 },
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 100,
          "unit": "percent"
        }
      },
      "targets": [
        {
          "expr": "(sum(awx_instance_consumed_capacity) / sum(awx_instance_capacity)) * 100",
          "refId": "A"
        }
      ]
    },
    {
      "type": "table",
      "title": "Instance Info",
      "gridPos": { "x": 6, "y": 12, "w": 18, "h": 8 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "awx_instance_info",
          "format": "table",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeSeries",
      "title": "Job Duration (avg 5m)",
      "gridPos": { "x": 0, "y": 20, "w": 12, "h": 8 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "avg_over_time(awx_job_duration_seconds[5m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "barGauge",
      "title": "Jobs by Type",
      "gridPos": { "x": 12, "y": 20, "w": 12, "h": 8 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(awx_instance_launch_type_total) by (launch_type)",
          "refId": "A"
        }
      ]
    },
    {
      "type": "barGauge",
      "title": "Jobs by User",
      "gridPos": { "x": 0, "y": 28, "w": 24, "h": 8 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(awx_jobs_total) by (user)",
          "refId": "A"
        }
      ]
    }
  ]
}


